<% provide(:title, @profile.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= @profile.name %>
      </h1>
      <p>
        <%= @profile.street %>,<br>
        <%= @profile.city %>
        <%= @profile.state %>,<br>
        <%= @profile.zipcode %><br><br>
        <%= @profile.user.email %>
      </p>
    </section>
  </aside>
</div>
