<% provide(:title, 'Edit Team') %>
<h1>Edit Team</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@team) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'fields', f: f %>    
     <%= f.submit "Save Changes", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>